<?xml version="1.0" encoding="UTF-8"?>

<web-app version="3.0">

    <welcome-file-list>

        <welcome-file>/login</welcome-file>

    </welcome-file-list>


    <context-param>

        <param-name>bookshelf.storageType</param-name>

        <param-value>cloudsql</param-value>

    </context-param>

    <context-param>

        <param-name>bookshelf.bucket</param-name>

        <param-value>untitledboardgame-261600</param-value>

    </context-param>

    <context-param>

        <param-name>sql.urlRemote</param-name>

        <param-value>jdbc:mysql://google/profiles?cloudSqlInstance=untitledboardgame-261600:us-central1:untitledboardgame&amp;socketFactory=com.google.cloud.sql.mysql.SocketFactory&amp;user=root&amp;password=myRootPassword1234</param-value>

    </context-param>

    <context-param>

        <param-name>sql.urlLocal</param-name>

        <param-value>jdbc:mysql://localhost/profiles?user=root&amp;password=myRootPassword1234&amp;useSSL=false</param-value>

    </context-param>

</web-app>
